section#contact.container-sec
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3330.0279721840757!2d-112.00106088447913!3d33.42251508078197!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x872b0ef88de354c9%3A0x52a0020c802f1a4e!2s3822%20E%20University%20Dr%2C%20Phoenix%2C%20AZ%2085034!5e0!3m2!1sen!2sus!4v1672350865236!5m2!1sen!2sus" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
    div.container
        div.contact-info
            h2.h3.fw-800
                span.primary Contact 
                span.secondary us
            p 
                | Call or text JAUS Property Services provider 24/7 for any questions.
                br
                strong.h5.fw-700.primary 602-877-2343
                br
                | or email
                br
                strong.h5.fw-700.primary info@jausproperty.com


        div.contact-form
            h2.h4.fw-800.my-30px.text-center
                span.primary Make An
                br
                span.secondary Appointment
            
            form
                div.d-block
                    label Name/Company*
                    input(type="text" name="name-or-company" required).input-control
                div.d-block
                    label Email*
                    input(type="email" name="email" required).input-control
                div.d-block
                    label Phone*
                    input(type="text" name="Phone" required).input-control
                div.d-block
                    label Message
                    textarea(name="message", cols="30", rows="10").input-control
                div.d-flex.justify-center.mt-50px.mb-20px
                    button.btn.btn-large.btn-tense.btn-primary-outlined.w-10 Submit
